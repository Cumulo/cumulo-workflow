{
  "name": "server",
  "version": "1.0.0",
  "description": "Boot Workflow ----",
  "main": "index.js",
  "scripts": {
    "tiye": "rsync -r dist/main.js repo.tiye.me:servers/tiye",
    "dev": "webpack --config webpack.dev.js --watch",
    "bundle": "webpack --progress",
    "cljs-once": "shadow-cljs --once",
    "cljs": "shadow-cljs --dev",
    "build": "yarn cljs-once && yarn bundle",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "shadow-cljs": {
    "source-paths": [
      "src"
    ],
    "output-dir": "compiled",
    "dependencies": [
      [
        "mvc-works/hsl",
        "0.1.2"
      ],
      [
        "cumulo/recollect",
        "0.1.7"
      ],
      [
        "org.clojure/core.async",
        "0.3.443"
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvc-works/boot-workflow.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mvc-works/boot-workflow/issues"
  },
  "homepage": "https://github.com/mvc-works/boot-workflow#readme",
  "dependencies": {
    "shortid": "^2.2.6",
    "uws": "^0.13.0"
  },
  "devDependencies": {
    "gaze": "^1.1.2",
    "shadow-cljs": "^0.6.0",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.6.1"
  }
}
