{
  "name": "cumulo-workflow",
  "version": "0.1.0",
  "description": "Stack Workflow",
  "main": "index.js",
  "scripts": {
    "tiye": "rsync -r dist/ tiye.me:repo/Cumulo/workflow",
    "cljs-release": "shadow-cljs -b app --release",
    "cljs-once": "shadow-cljs -b app --once",
    "cljs": "shadow-cljs -b app --dev",
    "html-dev": "env=dev node target/app.render.js",
    "html": "node target/app.render.js",
    "hmr": "webpack-dev-server --config webpack.dev.js --hot-only",
    "webpack": "webpack --hide-modules --progress",
    "dev": "yarn html-dev && yarn hmr",
    "del": "rm -rf dist/*",
    "serve": "http-server dist/",
    "build": "yarn del && yarn cljs-release && yarn webpack && yarn cljs-once && yarn html && yarn serve"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/stack-workflow.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "http-server": "^0.10.0",
    "respo-ui": "^0.1.2",
    "shadow-cljs": "^0.9.4",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
